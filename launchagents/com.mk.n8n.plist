    <key>ProgramArguments</key>
    <array>
      <string>/bin/bash</string>
      <string>-lc</string>
      <string>set -euo pipefail; source /Users/mk/.openclaw/secrets/secrets.env; exec /Users/mk/.nvm/versions/node/v20.20.0/bin/n8n</string>
